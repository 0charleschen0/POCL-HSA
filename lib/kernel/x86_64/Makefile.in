install-targetpkglibDATA: $(targetpkglib_DATA)
	@$(NORMAL_INSTALL)
	@list='$(targetpkglib_DATA)'; test -n "$(targetpkglibdir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(targetpkglibdir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(targetpkglibdir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(targetpkglibdir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(targetpkglibdir)" || exit $$?; \
	done

uninstall-targetpkglibDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(targetpkglib_DATA)'; test -n "$(targetpkglibdir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	dir='$(DESTDIR)$(targetpkglibdir)'; $(am__uninstall_files_from_dir)
