# Search the .cl, .c, and .ll sources first from this (target
# specific) directory, then the one-up (generic) directory. This
# allows to override the generic implementation simply by adding a
# similarly named file in the target specific directory

# NOTE: GNU make always searches from '.' first, which can be
# problematic in case one wants to override the files in the
# device-specific directory. To circumvent this, one can override a
# default file with a more accurate path, e.g.,
# vecmathlib/pocl/fmax.cl. This overrides a possible fmax.cl found in
# the current directory by one from the VML. TODO: dependency tracking
# does not work in that case because the .bc is not created in the
# directory where the source is.
@USE_VECMATHLIB_TRUE@vpath %.h @top_srcdir@/include:@srcdir@:${SECONDARY_VPATH}:@srcdir@/..
@USE_VECMATHLIB_TRUE@vpath %.c @srcdir@:${SECONDARY_VPATH}:@srcdir@/..
@USE_VECMATHLIB_TRUE@vpath %.cc @srcdir@:${SECONDARY_VPATH}:@srcdir@/..
@USE_VECMATHLIB_TRUE@vpath %.cl @srcdir@:${SECONDARY_VPATH}:@srcdir@/..
@USE_VECMATHLIB_TRUE@vpath %.ll @srcdir@:${SECONDARY_VPATH}:@srcdir@/..

# Rules to compile the different kernel library source file types into
# LLVM bitcode
@USE_VECMATHLIB_TRUE@%.c.bc: %.c @top_builddir@/include/${TARGET_DIR}/types.h
@USE_VECMATHLIB_TRUE@	@CLANG@ -Xclang -ffake-address-space-map -emit-llvm ${CLFLAGS} ${EXTRA_CLANGFLAGS} -c -target ${KERNEL_TARGET} -o ${notdir $@} -x c $< -include ../../../include/${TARGET_DIR}/types.h
@USE_VECMATHLIB_TRUE@%.cc.bc: %.cc @top_builddir@/include/${TARGET_DIR}/types.h
@USE_VECMATHLIB_TRUE@	@CLANGXX@ -Xclang -ffake-address-space-map -fno-exceptions -emit-llvm ${EXTRA_CLANGFLAGS} ${CLANGXX_FLAGS} -c -target ${KERNEL_TARGET} -o ${notdir $@} $< -include ../../../include/${TARGET_DIR}/types.h
@USE_VECMATHLIB_TRUE@%.cl.bc: %.cl @top_builddir@/include/${TARGET_DIR}/types.h @top_srcdir@/include/_kernel.h
@USE_VECMATHLIB_TRUE@	@CLANG@ -Xclang -ffake-address-space-map -emit-llvm ${CLFLAGS} ${EXTRA_CLANGFLAGS} -fsigned-char -c -target ${KERNEL_TARGET} -o ${notdir $@} -x cl $< -include ../../../include/${TARGET_DIR}/types.h -include ${abs_top_srcdir}/include/_kernel.h
@USE_VECMATHLIB_TRUE@%.ll.bc: %.ll
@USE_VECMATHLIB_TRUE@	@LLVM_AS@ -o $@ $<

# Optimize the bitcode library to speed up optimization times for the
# OpenCL kernels
@USE_VECMATHLIB_TRUE@kernel-${KERNEL_TARGET}.bc: ${OBJ}
@USE_VECMATHLIB_TRUE@	@LLVM_LINK@ ${notdir $^} -o - | @LLVM_OPT@ ${KERNEL_LIB_OPT_FLAGS} -O3 -o $@

